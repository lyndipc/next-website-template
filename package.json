{
  "name": "next-website-template",
  "version": "1.0.0",
  "description": "This is a Next.js website template that includes rate limiting setup using Redis and Upstash, in addition to Airtable integration.",
  "private": false,
  "keywords": [
    "typescript",
    "nextjs",
    "website",
    "template",
    "rate-limiting",
    "redis",
    "upstash",
    "airtable",
    "contact-form"
  ],
  "author": "Lyndi Castrejon <lyndipc@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:rate-limit": "jest tests/rate-limiting",
    "test:rate-limit:integration": "node tests/rate-limiting/integration.test.js",
    "customize": "node scripts/customize.js"
  },
  "dependencies": {
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "airtable": "^0.12.2",
    "dotenv": "^17.2.1",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
